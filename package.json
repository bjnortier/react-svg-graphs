{
  "name": "react-svg-graphs",
  "version": "0.2.1",
  "description": "React SVG Graphs",
  "keywords": [
    "React",
    "SVG",
    "Graph"
  ],
  "main": "lib/index.js",
  "scripts": {
    "unit": "mocha --require babel-core/register --recursive test/unit/**",
    "unit:watch": "npm run unit -- --watch",
    "functional": "cd test/functional && webpack-dev-server",
    "test": "npm run unit",
    "compile": "babel --optional runtime -d lib/ src/",
    "prepublishOnly": "npm run compile"
  },
  "author": "Ben Nortier <ben@bjnortier.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 8.11.3"
  },
  "peerDependencies": {
    "react": "16.x",
    "prop-types": "15.x"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "expect": "^23.4.0",
    "json-loader": "^0.5.7",
    "mocha": "^5.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "standard": "^11.0.1",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "round10": "^1.0.3"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "before",
      "beforeEach",
      "after",
      "afterEach"
    ]
  }
}
