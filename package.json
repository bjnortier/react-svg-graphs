    "eslint-plugin-standard": "^4.0.1",
    "expect": "^24.1.0",
    "json-loader": "^0.5.7",
    "mocha": "^10.2.0",
    "opn": "^5.4.0",
    "prop-types": "^15.6.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-resize-detector": "^4.2.1",
    "react-router-dom": "^5.1.2",
    "rimraf": "^2.6.3",
    "styled-components": "^4.1.2",
    "styled-normalize": "^8.0.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
